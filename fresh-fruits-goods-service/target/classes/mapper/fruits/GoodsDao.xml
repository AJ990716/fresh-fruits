<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fresh.fruits.dao.GoodsDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.fresh.fruits.pojo.GoodsEntity" id="goodsMap">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="caption" column="caption"/>
        <result property="image" column="image"/>
        <result property="price" column="price"/>
        <result property="source" column="source"/>
        <result property="category1Id" column="category1_id"/>
        <result property="category2Id" column="category2_id"/>
        <result property="category3Id" column="category3_id"/>
        <result property="typeTemplateId" column="type_template_id"/>
        <result property="status" column="status"/>
        <result property="isEnableSpec" column="is_enable_spec"/>
        <result property="isMarketable" column="is_marketable"/>
        <result property="isDelete" column="is_delete"/>
    </resultMap>


</mapper>